<?xml version="1.0" encoding="UTF-8"?>
<launch>
	<include file="$(find dynamic_robot_localization_tests)/launch/environments/crob_lab/jarvis/lab_jarvis.launch" >
		<arg name="world_rosbag_filename" default="$(find dynamic_robot_localization_tests)/datasets/crob_lab/jarvis/jarvis_complex_path_outliers_very_fast_stage.bag /base_scan:=/driver_laser_navigation/laser_scan /beacons_scan:=/driver_laser_navigation/beacons_scan" />
		<arg name="world_rosbag_results_filename" default="$(find dynamic_robot_localization_tests)/datasets/crob_lab/jarvis/jarvis_complex_path_outliers_very_fast_stage_simulator_map_results.bag" />
		<arg name="map_file" default="$(find dynamic_robot_localization_tests)/maps/crob_lab/planar/lab_slam_jarvis_stage.yaml" />
		<arg name="rviz_folder" default="$(find dynamic_robot_localization_tests)/rviz/jarvis/crob_lab_slam" />
		<arg name="robot_initial_pose_in_map_to_base" default="true" />
		<arg name="robot_initial_x" default="-18.259" />
		<arg name="robot_initial_y" default="-21.604" />
		<arg name="robot_initial_z" default="0.0" />
		<arg name="robot_initial_roll" default="0.0" />
		<arg name="robot_initial_pitch" default="0.0" />
		<arg name="robot_initial_yaw" default="0.069" />
		<arg name="ground_truth_correction_x" default="-13.0016739692" /> <!-- lower -> ground truth moves right -->
		<arg name="ground_truth_correction_y" default="-19.9684539223" /> <!-- lower -> ground truth moves up -->
		<arg name="ground_truth_correction_z" default="0.0" />
		<arg name="ground_truth_correction_roll" default="0.0" />
		<arg name="ground_truth_correction_pitch" default="0.0" />
		<arg name="ground_truth_correction_yaw" default="-3.14152161776" /> <!-- lower -> ground truth moves counter clockwise -->
	</include>
</launch>
